{
  "name": "starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@devexpress/dx-core": "^2.7.2",
    "@devexpress/dx-react-core": "^2.7.2",
    "@devexpress/dx-react-grid": "^2.7.2",
    "@devexpress/dx-react-grid-bootstrap4": "^2.7.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "heroicons-react": "^1.3.0",
    "mobx": "^5.15.6",
    "mobx-react-lite": "^2.2.1",
    "mobx-state-tree": "^3.17.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-loading-skeleton": "^2.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.4",
    "tailwindcss": "^1.9.5",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "build:tailwind": "tailwind build src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "start:react": "PORT=3333 react-scripts start",
    "prebuild": "run-s build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext=tsx,ts",
    "lint:fix": "eslint ./src --ext=tsx,ts --fix",
    "serve": "serve -l 3000 -s ./build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.21.5",
    "node-sass": "^4.14.1",
    "pre-commit": "^1.2.2",
    "chokidar-cli": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2"
  }
}
